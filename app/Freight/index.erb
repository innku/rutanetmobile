<div class="toolbar">
	<h1 id="pageTitle">
		Cargas
	</h1>
	<%= link_to "Contactos", :controller => 'Contact', :action => 'index' %>
</div>

<ul id="freights" title="Freights" selected="true">
	<%@freights.each do |freight|%>
		<li>
			<%=link_to "<strong>#{freight.category}</strong> <br /> #{freight.origin} -> #{freight.destination}", :action => :show, :id => freight.object %>
		</li>
	<% end %>
</ul>